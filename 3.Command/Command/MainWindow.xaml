<Window x:Class="Command.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Command"
        xmlns:qt="http://QtMvvm.com"
        mc:Ignorable="d" qt:ViewModelAutoWired.Value="True"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock FontSize="20" Text="{Binding Value}"></TextBlock>
        <Button Width="100" Height="100" Command="{Binding ValueChangeCommand}">Value Change</Button>
    </Grid>
</Window>
